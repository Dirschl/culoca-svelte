{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        { "type": "host", "value": "www.culoca.com" }
      ],
      "destination": "https://culoca.com/:path*",
      "permanent": true
    }
  ],
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": "sveltekit"
} 